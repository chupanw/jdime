public class Base {
  @Conditional static boolean patch2 = true;
  @Conditional static boolean patch1 = true;
  @Conditional static boolean patch0 = true;
  public static void main(String[] args) {
    int a = 1;
    int b = 2;
    int c;
    if(patch2) {
      a++;
    }
    if(!patch1 && !patch2) {
      c = 3;
    }
    if(patch2) {
      b = a;
    }
    if(patch1) {
      System.out.println(a);
    }
    if(patch0) {
      System.out.println(a);
    }
    if(!patch2) {
      System.out.println(b);
    }
    if(patch2) {
      assert b == 3;
    }
  }
}